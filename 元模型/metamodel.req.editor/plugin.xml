      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="metamodel.req.presentation.ReqModelWizardID"
            name="%_UI_ReqModelWizard_label"
